Options -Indexes
Options +FollowSymLinks
ServerSignature Off

<IfModule mod_rewrite.c>
RewriteEngine on

RewriteCond %{SCRIPT_FILENAME} !-d 
RewriteCond %{SCRIPT_FILENAME} !-f

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.*)$ $1.php

</IfModule>






